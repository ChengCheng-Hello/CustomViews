<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cc.custom">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.cc.custom.MainActivity"
            android:windowSoftInputMode="adjustNothing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".viewpager.TXVpDemoActivity" />
        <activity android:name=".calender.TXCalenderDemoActivity" />
        <activity android:name=".calender.TXCalenderListDemoActivity" />
        <activity android:name=".calender.demo.TXCalendarPickerActivity" />
        <activity android:name=".stepview.TXStepViewDemoActivity" />
        <activity android:name=".video.TXVideoEditDemoActivity" />
        <activity android:name=".ndk.TXNDKDemoActivity" />
    </application>

</manifest>